<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>SeedsPlease</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<SeedsPlease.SeedDef ParentName="SeedBase">
							<defName>Yadarit_seeds</defName>
							<label>Споры ядарита</label>
							<sources>
								<li>YadaritPlant</li>
							</sources>
							<graphicData>
								<texPath>Things/Item/Seed/Yadarit1</texPath>
							</graphicData>
							 <statBases>
                                <MarketValue>9</MarketValue>
                             </statBases>
						</SeedsPlease.SeedDef>
					</value>
				</li>	
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						<RecipeDef ParentName="ExtractSeed">
							<defName>ExtractYadaritseeds</defName>
							<label>Извлечение ядарит спор</label>
							<description>Извлечение спор ядарита</description>
							<ingredients>
								<li>
									<filter>
										<thingDefs>
											<li>yadaritRaW</li>
										</thingDefs>
									</filter>
									<count>4</count>
								</li>
							</ingredients>
							<fixedIngredientFilter>
								<thingDefs>
									<li>yadaritRaW</li>
								</thingDefs>
							</fixedIngredientFilter>
							<products>
								<Yadarit_seeds>3</Yadarit_seeds>
							</products>
						</RecipeDef>
					</value>
				</li>
									
			</operations>
		</match>
	</Operation>
</Patch>